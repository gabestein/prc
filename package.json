{
  "name": "expenses",
  "version": "1.0.0",
  "description": "A super simple expense tracker and budgeter.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabestein/budget.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "env NODE_ENV=development next",
    "sandbox": "env NODE_ENV=sandbox next",
    "build": "env NODE_ENV=production next build",
    "start": "env NODE_ENV=production next start",
    "console": "cd db && npx hasura console",
    "migrate:dev": "cd db && dotenv -e ../.env node migrate-dev.js",
    "deploy": "now",
    "deploy:prod": "now --local-config now.prod.json --prod",
    "deploy:dev": "now && now alias futureyou.gabestein.now.sh futureyou-dev.now.sh",
    "deploy:sandbox": "now --local-config=now.sandbox.json && now alias futureyou.gabestein.now.sh futureyou-sandbox.now.sh",
    "pretty": "prettier --write '**/*.js'",
    "lint": "eslint .",
    "test": "jest --watch"
  },
  "keywords": [],
  "author": "Gabriel Stein <gabe@ghscommunications.com>",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@auth0/nextjs-auth0": "^1.4.2",
    "@blueprintjs/core": "^3.22.3",
    "@types/react": "^16.9.17",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.7",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.24.0",
    "next": "^9.1.7",
    "next-compose-plugins": "^2.2.0",
    "next-with-apollo": "^4.3.0",
    "node-sass": "^4.13.0",
    "normalize": "^0.3.1",
    "plaid": "^4.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-plaid-link": "^1.5.0",
    "swr": "^0.1.16",
    "vercel": "^19.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^25.1.0",
    "dotenv-cli": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "hasura-cli": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}
